    "presets": [
        ["@babel/env", {
            "targets": {
                "node": "current"
            }
        }],
        ["@babel/typescript", {
            "targets": {
                "node": "current"
            }
        }]
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread"
    ]
}